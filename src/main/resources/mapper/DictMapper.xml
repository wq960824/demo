<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.DictMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.demo.entity.Dict">
        <id column="dict_id" property="dictId" />
        <result column="type" property="type" />
        <result column="code" property="code" />
        <result column="code_ex" property="codeEx" />
        <result column="value" property="value" />
        <result column="sort" property="sort" />
        <result column="time_h" property="timeH" />
        <result column="time_l" property="timeL" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        dict_id, type, code, code_ex, value, sort, time_h, time_l
    </sql>

</mapper>
