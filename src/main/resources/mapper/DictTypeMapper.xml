<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.DictTypeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.demo.entity.DictType">
        <id column="type_id" property="typeId" />
        <result column="type" property="type" />
        <result column="name" property="name" />
        <result column="time_h" property="timeH" />
        <result column="time_l" property="timeL" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        type_id, type, name, time_h, time_l
    </sql>

</mapper>
